<%@ page contentType="text/html;charset=UTF-8" %>
<%@ include file="/webpage/include/taglib.jsp"%>
<html>
<head>
	<title>订单详情管理</title>
	<meta name="decorator" content="default"/>
	<script type="text/javascript">
		$(document).ready(function() {
		});
	</script>
</head>
<body class="gray-bg">
	<div class="wrapper wrapper-content">
	<div class="ibox">
	<div class="ibox-title">
		<h5>订单详情列表 </h5>
		<div class="ibox-tools">
			<a class="collapse-link">
				<i class="fa fa-chevron-up"></i>
			</a>
			<a class="dropdown-toggle" data-toggle="dropdown" href="#">
				<i class="fa fa-wrench"></i>
			</a>
			<ul class="dropdown-menu dropdown-user">
				<li><a href="#">选项1</a>
				</li>
				<li><a href="#">选项2</a>
				</li>
			</ul>
			<a class="close-link">
				<i class="fa fa-times"></i>
			</a>
		</div>
	</div>
    
    <div class="ibox-content">
	<sys:message content="${message}"/>
	
	<!--查询条件-->
	<div class="row">
	<div class="col-sm-12">
	<form:form id="searchForm" modelAttribute="orderDetail" action="${ctx}/orderdetail/orderDetail/" method="post" class="form-inline">
		<input id="pageNo" name="pageNo" type="hidden" value="${page.pageNo}"/>
		<input id="pageSize" name="pageSize" type="hidden" value="${page.pageSize}"/>
		<table:sortColumn id="orderBy" name="orderBy" value="${page.orderBy}" callback="sortOrRefresh();"/><!-- 支持排序 -->
		<div class="form-group">
		 </div>	
	</form:form>
	<br/>
	</div>
	</div>
	
	<!-- 工具栏 -->
	<div class="row">
	<div class="col-sm-12">
		<div class="pull-left">
			<%-- <shiro:hasPermission name="orderdetail:orderDetail:add">
				<table:addRow url="${ctx}/orderdetail/orderDetail/form" title="订单详情"></table:addRow><!-- 增加按钮 -->
			</shiro:hasPermission>
			<shiro:hasPermission name="orderdetail:orderDetail:edit">
			    <table:editRow url="${ctx}/orderdetail/orderDetail/form" title="订单详情" id="contentTable"></table:editRow><!-- 编辑按钮 -->
			</shiro:hasPermission>
			<shiro:hasPermission name="orderdetail:orderDetail:del">
				<table:delRow url="${ctx}/orderdetail/orderDetail/deleteAll" id="contentTable"></table:delRow><!-- 删除按钮 -->
			</shiro:hasPermission> --%>
			<%-- <shiro:hasPermission name="orderdetail:orderDetail:import">
				<table:importExcel url="${ctx}/orderdetail/orderDetail/import"></table:importExcel><!-- 导入按钮 -->
			</shiro:hasPermission>
			<shiro:hasPermission name="orderdetail:orderDetail:export">
	       		<table:exportExcel url="${ctx}/orderdetail/orderDetail/export"></table:exportExcel><!-- 导出按钮 -->
	       	</shiro:hasPermission> --%>
			</div>
		<div class="pull-right">
		</div>
	</div>
	</div>
	
	<!-- 表格 -->
	<table id="contentTable" class="table table-striped table-bordered table-hover table-condensed dataTables-example dataTable">
		<thead>
			<tr>
				<th> <input type="checkbox" class="i-checks"></th>
				<th  class="sort-column xhOrder.id">订单号</th>
				<th  class="sort-column xhGoods.id">商品名称</th>
				<th  class="sort-column num">商品数量</th>
				<th  class="sort-column price">商品单价</th>
				<th  class="sort-column detail">商品详情</th>
				<th  class="sort-column mTime">包月时长</th>
				<th  class="sort-column endDate">到期时间</th>
				<th  class="sort-column status">商品类型</th>
				<th>操作</th>
			</tr>
		</thead>
		<tbody>
		<c:forEach items="${page.list}" var="orderDetail">
			<tr>
				<td> <input type="checkbox" id="${orderDetail.id}" class="i-checks"></td>
				<td><!-- <a  href="#" onclick="openDialogView('查看订单详情', '#','800px', '500px')"> -->
					${orderDetail.xhOrder.id}
				<!-- </a> --></td>
				<td>
					${orderDetail.gName}
				</td>
				<td>
					${orderDetail.num}
				</td>
				<td>
					${orderDetail.price}
				</td>
				<td>
					${orderDetail.info}
				</td>
				<td>
					${orderDetail.mTime}
				</td>
				<td>
					${orderDetail.endDate}
				</td>
				<td>
					${fns:getDictLabel(orderDetail.status, 'orderDetail', '')}
				</td>
				<td>
					<%-- <shiro:hasPermission name="orderdetail:orderDetail:view">
						<a href="#" onclick="openDialogView('查看订单详情', '${ctx}/orderdetail/orderDetail/form?id=${orderDetail.id}','800px', '500px')" class="btn btn-info btn-xs" ><i class="fa fa-search-plus"></i> 查看</a>
					</shiro:hasPermission>
					<shiro:hasPermission name="orderdetail:orderDetail:edit">
    					<a href="#" onclick="openDialog('修改订单详情', '${ctx}/orderdetail/orderDetail/form?id=${orderDetail.id}','800px', '500px')" class="btn btn-success btn-xs" ><i class="fa fa-edit"></i> 修改</a>
    				</shiro:hasPermission>
    				<shiro:hasPermission name="orderdetail:orderDetail:del">
						<a href="${ctx}/orderdetail/orderDetail/delete?id=${orderDetail.id}" onclick="return confirmx('确认要删除该订单详情吗？', this.href)"   class="btn btn-danger btn-xs"><i class="fa fa-trash"></i> 删除</a>
					</shiro:hasPermission> --%>
				</td>
			</tr>
		</c:forEach>
		</tbody>
	</table>
	
		<!-- 分页代码 -->
	<table:page page="${page}"></table:page>
	<br/>
	<br/>
	</div>
	</div>
</div>
</body>
</html>